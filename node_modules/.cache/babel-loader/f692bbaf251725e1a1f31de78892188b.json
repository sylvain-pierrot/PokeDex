            .then((json) => {  \n                console.log(json.results)\n                this.currentPokemon = json.results\n            })\n            .catch((err) => {  console.error(`Fetch problem: ${err.message}`)})\n        }\n    },\n    created() {\n        this.fetchCurrentPokemon()\n    }\n}\n</script>\n\n<style>\n.pokemon-card {\n    width: 200px;\n    height: 500px;\n    background-color: red;\n}\n</style>\n"]},"metadata":{},"sourceType":"module"}