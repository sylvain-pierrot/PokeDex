         }),\n          minLevel: poke.minLevel\n        };\n      }));\n      pokeVarieties.value = await Promise.all(props.pokemon.varieties.map(async varietie => {\n        return await store.dispatch('createPokemon', {\n          name: varietie,\n          minimal: true\n        });\n      }));\n    });\n    const __returned__ = {\n      props,\n      router,\n      store,\n      pokeEvolves,\n      pokeVarieties,\n      varieteKey,\n      name,\n      types,\n      color,\n      image,\n      legendary,\n      description,\n      specifications,\n      abilities,\n      moves,\n      varietieChange,\n      ref,\n      watch,\n      onMounted,\n      useStore,\n      useRouter\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":";AAgEA,SAASA,GAAG,EAAEC,KAAkB,EAAEC,SAAS,QAAQ,KAAK;AACxD,SAASC,QAAQ,QAAQ,MAAM;AAC/B,SAASC,SAAS,QAAQ,YAAY;;;;;;;;;;;;;;IAStC,MAAMC,MAAM,GAAGD,SAAS,EAAE;IAC1B,MAAME,KAAK,GAAGH,QAAQ,EAAE;IACxB,MAAMI,WAAW,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC3B,MAAMQ,aAAa,GAAGR,GAAG,CAAC,EAAE,CAAC;IAC7B,MAAMS,UAAU,GAAGT,GAAG,CAAC,EAAE,CAAC;;IAE1B;IACA,MAAMU,IAAI,GAAGV,GAAG,CAACW,KAAK,CAACC,OAAO,CAACF,IAAI,CAAC;IACpC,MAAMG,KAAK,GAAGb,GAAG,CAACW,KAAK,CAACC,OAAO,CAACC,KAAK,CAAC;IACtC,MAAMC,KAAK,GAAGd,GAAG,CAACW,KAAK,CAACC,OAAO,CAACE,KAAK,CAAC;IACtC,MAAMC,KAAK,GAAGf,GAAG,CAACW,KAAK,CAACC,OAAO,CAACG,KAAK,CAAC;IACtC,MAAMC,SAAS,GAAGhB,GAAG,CAACW,KAAK,CAACC,OAAO,CAACI,SAAS,CAAC;IAC9C,MAAMC,WAAW,GAAGjB,GAAG,CAACW,KAAK,CAACC,OAAO,CAACK,WAAW,CAAC;IAClD,MAAMC,cAAc,GAAGlB,GAAG,CAACW,KAAK,CAACC,OAAO,CAACM,cAAc,CAAC;IACxD,MAAMC,SAAS,GAAGnB,GAAG,CAACW,KAAK,CAACC,OAAO,CAACO,SAAS,CAAC;IAC9C,MAAMC,KAAK,GAAGpB,GAAG,CAACW,KAAK,CAACC,OAAO,CAACQ,KAAK,CAAC;IAEtCnB,KAAK,CAAC,MAAMU,KAAK,CAACC,OAAO,EAAE,MAAM;MAC7BF,IAAI,CAACW,KAAK,GAAGV,KAAK,CAACC,OAAO,CAACF,IAAI;MAC/BG,KAAK,CAACQ,KAAK,GAAGV,KAAK,CAACC,OAAO,CAACC,KAAK;MACjCC,KAAK,CAACO,KAAK,GAAGV,KAAK,CAACC