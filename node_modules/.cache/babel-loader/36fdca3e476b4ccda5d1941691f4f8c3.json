{"ast":null,"code":"import PokemonCard from '../components/PokemonCard.vue';\nexport default {\n  name: 'PokemonList',\n  components: {\n    PokemonCard\n  },\n  data() {\n    return {\n      pokemons: []\n    };\n  },\n  computed: {\n    pokes() {\n      const Pokedex = require(\"pokeapi-js-wrapper\");\n      const P = new Pokedex.Pokedex();\n      const interval = {\n        offset: 0,\n        limit: 10\n      };\n      const pokes = {};\n      P.getPokemonsList(interval).then(function (response) {\n        response.results.forEach(pokemon => {\n          P.getPokemonByName(pokemon.name).then(function (pokemonDetail) {\n            // console.log(pokemonDetail)\n            pokes.push(pokemonDetail);\n            console.log(pokes);\n          });\n        });\n      });\n      return pokes;\n    }\n  }\n};","map":{"version":3,"mappings":"AASA,OAAOA,WAAU,MAAO,+BAA8B;AAEtD,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,QAAQ,EAAE;IACNC,KAAK,GAAG;MACJ,MAAMC,OAAM,GAAIC,OAAO,CAAC,oBAAoB;MAC5C,MAAMC,IAAI,IAAIF,OAAO,CAACA,OAAO,EAAC;MAC9B,MAAMG,QAAO,GAAI;QACbC,MAAM,EAAE,CAAC;QACTC,KAAK,EAAE;MACX;MACA,MAAMN,KAAI,GAAI,CAAC;MACfG,CAAC,CAACI,eAAe,CAACH,QAAQ,CAAC,CAACI,IAAI,CAAC,UAASC,QAAQ,EAAE;QAChDA,QAAQ,CAACC,OAAO,CAACC,OAAO,CAACC,OAAM,IAAK;UAChCT,CAAC,CAACU,gBAAgB,CAACD,OAAO,CAACjB,IAAI,CAAC,CAACa,IAAI,CAAC,UAASM,aAAa,EAAE;YAC1D;YACAd,KAAK,CAACe,IAAI,CAACD,aAAa;YACxBE,OAAO,CAACC,GAAG,CAACjB,KAAK;UACrB,CAAC;QAEL,CAAC,CAAC;MACN,CAAC;MACD,OAAOA,KAAI;IACf;EACJ;AACJ","names":["PokemonCard","name","components","data","pokemons","computed","pokes","Pokedex","require","P","interval","offset","limit","getPokemonsList","then","response","results","forEach","pokemon","getPokemonByName","pokemonDetail","push","console","log"],"sourceRoot":"","sources":["/home/pierrots/polytech/WOA/js/lab4/pokedex/src/views/PokemonList.vue"],"sourcesContent":["<template>\n    <ul>\n        <li>\n            <PokemonCard v-for=\"(pokemon, index) in pokemons\" :key=\"index\" :pokemon=\"pokemon\" />\n        </li>\n    </ul>\n</template>\n\n<script>\nimport PokemonCard from '../components/PokemonCard.vue'\n\nexport default {\n    name: 'PokemonList',\n    components: {\n        PokemonCard\n    },\n    data() {\n        return {\n            pokemons: []\n        }\n    },\n    computed: {\n        pokes() {\n            const Pokedex = require(\"pokeapi-js-wrapper\")\n            const P = new Pokedex.Pokedex()\n            const interval = {\n                offset: 0,\n                limit: 10,\n            }\n            const pokes = {}\n            P.getPokemonsList(interval).then(function(response) {\n                response.results.forEach(pokemon => {\n                    P.getPokemonByName(pokemon.name).then(function(pokemonDetail) {\n                        // console.log(pokemonDetail)\n                        pokes.push(pokemonDetail)\n                        console.log(pokes)\n                    })\n                    \n                });\n            })\n            return pokes\n        }\n    }\n}\n</script>\n\n<style>\n\n</style> "]},"metadata":{},"sourceType":"module"}