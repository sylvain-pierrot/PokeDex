Pokemon\", {\n          name: varietie,\n          minimal: true\n        });\n      }));\n    });\n    function varietieChange() {\n      console.log(varieteId.value);\n      router.push({\n        name: \"Pokemon\",\n        params: {\n          id: varieteId.value\n        }\n      });\n    }\n    onMounted(async () => {\n      pokeEvolves.value = await Promise.all(props.pokemon.evolutions.map(async poke => {\n        return {\n          poke: await store.dispatch(\"createPokemon\", {\n            name: poke.name,\n            minimal: true\n          }),\n          minLevel: poke.minLevel\n        };\n      }));\n      pokeVarieties.value = await Promise.all(props.pokemon.varieties.map(async varietie => {\n        return await store.dispatch(\"createPokemon\", {\n          name: varietie,\n          minimal: true\n        });\n      }));\n    });\n    const __returned__ = {\n      props,\n      router,\n      store,\n      varieteId,\n      id,\n      name,\n      types,\n      color,\n      image,\n      legendary,\n      description,\n      specifications,\n      abilities,\n      moves,\n      pokeEvolves,\n      pokeVarieties,\n      varietieChange,\n      ref,\n      watch,\n      onMounted,\n      useStore,\n      useRouter\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":";AA2FA,SAASA,GAAG,EAAEC,KAAkB,EAAEC,SAAS,QAAQ,KAAK;AACxD,SAASC,QAAQ,QAAQ,MAAM;AAC/B,SAASC,SAAS,QAAQ,YAAY;;;;;;;;;;;;;;IAStC,MAAMC,MAAM,GAAGD,SAAS,EAAE;IAC1B,MAAME,KAAK,GAAGH,QAAQ,EAAE;IACxB,MAAMI,SAAS,GAAGP,GAAG,CAAC,EAAE,CAAC;IAEzB,MAAMQ,EAAE,GAAGR,GAAG,CAACS,KAAK,CAACC,OAAO,CAACF,EAAE,CAAC;IAChC,MAAMG,IAAI,GAAGX,GAAG,CAACS,KAAK,CAACC,OAAO,CAACC,IAAI,CAAC;IACpC,MAAMC,KAAK,GAAGZ,GAAG,CAACS,KAAK,CAACC,OAAO,CAACE,KAAK,CAAC;IACtC,MAAMC,KAAK,GAAGb,GAAG,CAACS,KAAK,CAACC,OAAO,CAACG,KAAK,CAAC;IACtC,MAAMC,KAAK,GAAGd,GAAG,CAACS,KAAK,CAACC,OAAO,CAACI,KAAK,CAA