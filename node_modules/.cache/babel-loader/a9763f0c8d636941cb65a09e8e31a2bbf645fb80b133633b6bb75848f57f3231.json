{"ast":null,"code":"import { ref, computed, onUnmounted, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport PokeCard from '../components/PokeCard.vue';\nimport { watch } from 'vue';\nexport default {\n  __name: 'PokedexPage',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const searchTerm = ref('');\n    const store = useStore();\n    const pokemons = computed(() => {\n      return store.state.pokemons;\n    });\n    const matchs = computed(() => {\n      return store.state.matchs;\n    });\n    window.addEventListener('scroll', generateMorePokemons);\n    async function generateMorePokemons() {\n      if (window.scrollY >= window.scrollMaxY) {\n        await store.dispatch('generatePokedex');\n      }\n    }\n    function generatePokedexBySearch(name) {\n      searchTerm.value = name;\n      const evt = new CustomEvent(\"submit\", {\n        \"bubbles\": true,\n        \"cancelable\": true\n      });\n      document.getElementById('form').dispatchEvent(evt);\n    }\n    async function onSubmit(event) {\n      event.preventDefault();\n      if (searchTerm.value === '') {\n        window.addEventListener('scroll', generateMorePokemons);\n      } else {\n        window.removeEventListener('scroll', generateMorePokemons);\n      }\n      await store.dispatch('generatePokedexBySearch', searchTerm.value);\n    }\n    async function generateLegendary() {\n      await store.dispatch('generateAllLegendary');\n      window.removeEventListener('scroll', generateMorePokemons);\n    }\n    let timer = null;\n    watch(searchTerm, name => {\n      if (timer) {\n        clearTimeout(timer);\n        timer = null;\n      }\n      timer = setTimeout(async () => {\n        await store.dispatch('generateMatchsPokemons', name);\n      }, 255);\n    });\n    onMounted(async () => {\n      await store.dispatch('generatePokedex');\n      // await store.dispatch('initCacheNames')\n      for (const key in pokemons) {\n        saveStorage(key, store.state.pokemons[0][key]);\n      }\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', generateMorePokemons);\n    });\n    const saveStorage = function (key, data) {\n      localStorage.setItem(key, JSON.stringify(data));\n    };\n    const getStorage = function (key, item) {\n      if (localStorage.getItem(key) && item) {\n        const data = JSON.parse(localStorage.getItem(key));\n        return data[item];\n      } else if (localStorage.getItem(key)) {\n        return localStorage.getItem(key);\n      }\n    };\n    const clearStorage = function (key = 'false') {\n      if (key) {\n        localStorage.removeItem(key);\n      } else {\n        localStorage.clear();\n      }\n    };\n    const __returned__ = {\n      searchTerm,\n      store,\n      pokemons,\n      matchs,\n      generateMorePokemons,\n      generatePokedexBySearch,\n      onSubmit,\n      generateLegendary,\n      timer,\n      saveStorage,\n      getStorage,\n      clearStorage,\n      ref,\n      computed,\n      onUnmounted,\n      onMounted,\n      useStore,\n      PokeCard,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AA6BA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,WAAW,EAAEC,SAAS,QAAQ,KAAK;AAC3D,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,SAASC,KAAK,QAAQ,KAAK;;;;;;;IAE3B,MAAMC,UAAU,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMQ,KAAK,GAAGJ,QAAQ,EAAE;IACxB,MAAMK,QAAQ,GAAGR,QAAQ,CAAC,MAAM;MAAG,OAAOO,KAAK,CAACE,KAAK,CAACD,QAAQ;IAAA,CAAC,CAAC;IAChE,MAAME,MAAM,GAAGV,QAAQ,CAAC,MAAM;MAAG,OAAOO,KAAK,CAACE,KAAK,CAACC,MAAM;IAAA,CAAC,CAAC;IAE5DC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAEC,oBAAoB,CAAC;IAEvD,eAAeA,oBAAoB,GAAG;MAClC,IAAIF,MAAM,CAACG,OAAO,IAAIH,MAAM,CAACI,UAAU,EAAE;QAAE,MAAMR,KAAK,CAACS,QAAQ,CAAC,iBAAiB,CAAC;MAAA;IACtF;IACA,SAASC,uBAAuB,CAACC,IAAI,EAAE;MACnCZ,UAAU,CAACa,KAAK,GAAGD,IAAI;MACvB,MAAME,GAAG,GAAG,IAAIC,WAAW,CAAC,QAAQ,EAAE;QAAC,S