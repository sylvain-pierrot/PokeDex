{"ast":null,"code":"import { ref, computed, onUnmounted, onMounted } from 'vue';\nimport { useStore } from 'vuex';\nimport PokeCard from '../components/PokeCard.vue';\nimport { watch } from 'vue';\nexport default {\n  __name: 'PokedexPage',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const searchTerm = ref('');\n    const timer = ref(null);\n    const store = useStore();\n    const pokemons = computed(() => {\n      return store.state.pokemons;\n    });\n    const matchs = computed(() => {\n      return store.state.matchs;\n    });\n    async function generateMorePokemons() {\n      if (window.scrollY >= window.scrollMaxY) {\n        await store.dispatch('generatePokedex', false);\n      }\n    }\n    function generatePokedexBySearch(name) {\n      searchTerm.value = name;\n      const evt = new CustomEvent(\"submit\", {\n        \"bubbles\": true,\n        \"cancelable\": true\n      });\n      document.getElementById('form').dispatchEvent(evt);\n    }\n    async function onSubmit(event) {\n      event.preventDefault();\n      if (searchTerm.value === '') {\n        window.addEventListener('scroll', generateMorePokemons);\n      } else {\n        window.removeEventListener('scroll', generateMorePokemons);\n      }\n      await store.dispatch('generatePokedexBySearch', searchTerm.value);\n    }\n    async function generateLegendary() {\n      await store.dispatch('generateAllLegendary');\n      window.removeEventListener('scroll', generateMorePokemons);\n    }\n    watch(searchTerm, name => {\n      if (timer.value) {\n        clearTimeout(timer.value);\n        timer.value = null;\n      }\n      timer.value = setTimeout(async () => {\n        await store.dispatch('generateMatchsPokemons', name);\n      }, 255);\n    });\n    onMounted(async () => {\n      await store.dispatch('generatePokedex', true);\n      window.addEventListener('scroll', generateMorePokemons);\n    });\n    onUnmounted(() => {\n      window.removeEventListener('scroll', generateMorePokemons);\n    });\n    const __returned__ = {\n      searchTerm,\n      timer,\n      store,\n      pokemons,\n      matchs,\n      generateMorePokemons,\n      generatePokedexBySearch,\n      onSubmit,\n      generateLegendary,\n      ref,\n      computed,\n      onUnmounted,\n      onMounted,\n      useStore,\n      PokeCard,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AA6BA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,WAAW,EAAEC,SAAS,QAAQ,KAAK;AAC3D,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,SAASC,KAAK,QAAQ,KAAK;;;;;;;IAE3B,MAAMC,UAAU,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMQ,KAAK,GAAGR,GAAG,CAAC,IAAI,CAAC;IACvB,MAAMS,KAAK,GAAGL,QAAQ,EAAE;IACxB,MAAMM,QAAQ,GAAGT,QAAQ,CAAC,MAAM;MAAG,OAAOQ,KAAK,CAACE,KAAK,CAACD,QAAQ;IAAA,CAAC,CAAC;IAChE,MAAME,MAAM,GAAGX,QAAQ,CAAC,MAAM;MAAG,OAAOQ,KAAK,CAACE,KAAK,CAACC,MAAM;IAAA,CAAC,CAAC;IAE5D,eAAeC,oBAAoB,GAAG;MAClC,IAAIC,MAAM,CAACC,OAAO,IAAID,MAAM,CAACE,UAAU,EAAE;QACrC,MAAMP,KAAK,CAACQ,QAAQ,CAAC,iBAAiB,EAAE,KAAK,CAAC;MAClD;IACJ;IACA,SAASC,uBAAuB,CAACC,IAAI,EAAE;MACnCZ,UAAU,CAACa,KAAK,GAAGD,IAAI;MACvB,MAAME,GAAG,GAAG,IAAIC,WAAW,CAAC,QAAQ,EAAE;QAAC,SAAS,EAAE,IAAI;QAAE,YAAY,EAAE;MAAI,CAAC,CAAC;MAC5EC,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,aAAa,CAACJ,GAAG,CAAC;IACtD;IACA,eAAeK,QAAQ,CAACC,KAAK,EAAE;MAC3BA,KAAK,CAACC,cAAc,EAAE;MACtB,IAAIrB,UAAU,CAACa,KAAK,KAAK,EAAE,EAAE;QACzBN,MAAM,CAACe,gBAAgB,CAAC,QAAQ,EAAEhB,oBAAoB,CAAC;MAC3D,CAAC,MAAM;QACHC,MAAM,CAACgB,mBAAmB,CAAC,QAAQ,EAAEjB,oBAAoB,CAAC;MAC9D;MACA,MAAMJ,KAAK,CAACQ,QAAQ,CAAC,yBAAyB,EAAEV,UAAU,CAACa,KAAK,CAAC;IACrE;IACA,eAAeW,iBAAiB,GAAG;MAC/B,MAAMtB,KAAK,CAACQ,QAAQ,CAAC,sBAAsB,CAAC;MAC5CH,MAAM,CAACgB,mBAAmB,CAAC,QAAQ,EAAEjB,oBAAoB,CAAC;IAC9D;IAEAP,KAAK,CAACC,UAAU,EAAGY,IAAI,IAAK;MACxB,IAAIX,KAAK,CAACY,KAAK,EAAE;QACbY,YAAY,CAACxB,KAAK,CAACY,KAAK,CAAC;QACzBZ,KAAK,CAACY,KAAK,GAAG,IAAI;MACtB;MACAZ,KAAK,CAACY,KAAK,GAAGa,UAAU,CAAC,YAAY