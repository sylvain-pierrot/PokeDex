{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PokemonCard from '@/components/PokemonCard.vue';\n// import Pokedex from 'pokeapi-js-wrapper'\n\nexport default {\n  name: 'PokemonList',\n  components: {\n    PokemonCard\n  },\n  data() {\n    return {\n      pokemons: [],\n      apiURL: 'https://pokeapi.co/api/v2/pokemon/'\n    };\n  },\n  methods: {\n    fetchPokemon() {\n      // const Pokedex = require(\"pokeapi-js-wrapper\")\n      // const P = new Pokedex.Pokedex()\n      // P.getPokemonByName(\"eevee\").then(function(response) {\n      //   console.log(response)\n      // })\n\n      fetch(this.apiURL).then(reponse => {\n        if (!reponse.ok) {\n          throw new Error(`HTTP error: ${reponse.status}`);\n        }\n        return reponse.json();\n      }).then(json => {\n        console.log(json.results);\n        for (const pokemon of json.results) {\n          this.pokemons.push(pokemon);\n        }\n      }).catch(err => {\n        console.error(`Fetch problem: ${err.message}`);\n      });\n    }\n  },\n  created() {\n    this.fetchPokemon();\n  }\n};","map":{"version":3,"mappings":";AASA,OAAOA,WAAU,MAAO,8BAA6B;AACrD;;AAEA,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE,EAAE;MACZC,MAAM,EAAE;IACZ;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,YAAY,GAAG;MACX;MACA;MACA;MACA;MACA;;MAEAC,KAAK,CAAC,IAAI,CAACH,MAAM,EAChBI,IAAI,CAAEC,OAAO,IAAK;QACf,IAAI,CAACA,OAAO,CAACC,EAAE,EAAE;UACb,MAAM,IAAIC,KAAK,CAAE,eAAcF,OAAO,CAACG,MAAO,EAAC;QACnD;QACA,OAAOH,OAAO,CAACI,IAAI,EAAC;MACxB,CAAC,EACAL,IAAI,CAAEK,IAAI,IAAK;QACZC,OAAO,CAACC,GAAG,CAACF,IAAI,CAACG,OAAO;QACxB,KAAK,MAAMC,OAAM,IAAKJ,IAAI,CAACG,OAAO,EAAE;UAChC,IAAI,CAACb,QAAQ,CAACe,IAAI,CAACD,OAAO;QAC9B;MACJ,CAAC,EACAE,KAAK,CAAEC,GAAG,IAAK;QAAGN,OAAO,CAACO,KAAK,CAAE,kBAAiBD,GAAG,CAACE,OAAQ,EAAC,CAAC;MAAA,CAAC;IACtE;EACJ,CAAC;EACDC,OAAO,GAAG;IACN,IAAI,CAACjB,YAAY,EAAC;EACtB;AACJ","names":["PokemonCard","name","components","data","pokemons","apiURL","methods","fetchPokemon","fetch","then","reponse","ok","Error","status","json","console","log","results","pokemon","push","catch","err","error","message","created"],"sourceRoot":"","sources":["/home/pierrots/polytech/WOA/js/lab4/pokedex/src/views/PokemonList.vue"],"sourcesContent":["<template>\n    <ul>\n        <li>\n            <PokemonCard v-for=\"(pokemon, index) in pokemons\" :key=\"index\" :pokemon=\"pokemon\" />\n        </li>\n    </ul>\n</template>\n\n<script>\nimport PokemonCard from '@/components/PokemonCard.vue'\n// import Pokedex from 'pokeapi-js-wrapper'\n\nexport default {\n    name: 'PokemonList',\n    components: {\n        PokemonCard\n    },\n    data() {\n        return {\n            pokemons: [],\n            apiURL: 'https://pokeapi.co/api/v2/pokemon/'\n        }\n    },\n    methods: {\n        fetchPokemon() {\n            // const Pokedex = require(\"pokeapi-js-wrapper\")\n            // const P = new Pokedex.Pokedex()\n            // P.getPokemonByName(\"eevee\").then(function(response) {\n            //   console.log(response)\n            // })\n\n            fetch(this.apiURL)\n            .then((reponse) => {\n                if (!reponse.ok) {\n                    throw new Error(`HTTP error: ${reponse.status}`)\n                }\n                return reponse.json()\n            })\n            .then((json) => {  \n                console.log(json.results)\n                for (const pokemon of json.results) {\n                    this.pokemons.push(pokemon)\n                }\n            })\n            .catch((err) => {  console.error(`Fetch problem: ${err.message}`)})\n        }\n    },\n    created() {\n        this.fetchPokemon()\n    }\n}\n</script>\n\n<style>\n\n</style>"]},"metadata":{},"sourceType":"module"}