erateMorePokemons,\n      loadOnScroll,\n      ref,\n      computed,\n      useStore,\n      PokeCard,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AAgCA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,SAASC,KAAK,QAAQ,KAAK;;;;;;;;IAJ3B;IAMA,MAAMC,UAAU,GAAGL,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMM,MAAM,GAAGN,GAAG,CAAC,IAAI,CAAC;IACxB,MAAMO,KAAK,GAAGL,QAAQ,EAAE;IACxB,MAAMM,QAAQ,GAAGP,QAAQ,CAAC,MAAM;MAAG,OAAOM,KAAK,CAACE,KAAK,CAACD,QAAQ;IAAA,CAAC,CAAC;IAChE,MAAME,MAAM,GAAGT,QAAQ,CAAC,MAAM;MAAG,OAAOM,KAAK,CAACE,KAAK,CAACC,MAAM;IAAA,CAAC,CAAC;IAE5DH,KAAK,CAACI,QAAQ,CAAC,kBAAkB,CAAC;IAClC;;IAEA,SAASC,oBAAoB,GAAG;MAC5BN,MAAM,CAACO,KAAK,GAAG,KAAK;MACpBN,KAAK,CAACI,QAAQ,CAAC,kBAAkB,CAAC;MAClCG,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAEC,YAAY,CAAC;IACnD;IACA,SAASA,YAAY,GAAG;MACpB,IAAIF,MAAM,CAACG,OAAO,IAAIH,MAAM,CAACI,UAAU,EAAE;QACrCN,oBAAoB,EAAE;MAC1B;IACJ;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;IACA;;IAEAR,KAAK,CAACC,UAAU,EAAGc,IAAI,IAAK;MACxB,IAAIA,IAAI,KAAK,EAAE,EAAE;QACb;MAAA,CACH,MAAM;QACHZ,KAAK,CAACI,QAAQ,CAAC,wBAAwB,EAAEQ,IAAI,CAAC;MAClD;IACJ,CAAC,CAAC;;IAEF;IACA","names":["ref","computed","useStore","PokeCard","watch","searchTerm","active","store","pokemons","state","matchs","dispatch","generateMorePokemons","value","window","addEventListener","loadOnScroll","scrollY","scrollMaxY","name"],"sources":["/home/pierrots/Polytech/WOA/js/lab4/pokedex/src/views/HomePage.vue"],"sourcesContent":["<template>\n    <div class=\"pokedex\">\n        <section class=\"section pokedex-header\">\n            <h1>POKEDEX</h1>\n        </section>\n        <section class=\"section pokedex-filter\">\n            <form v-on:submit.prevent=\"onSubmit\">\n                <div>\n                    <label for=\"search\">Quel Pokemon cherchez-vous ?</label>\n                    <input v-model=\"searchTerm\" type=\"text\" id=\"search\" name=\"search\" placeholder=\"Nom ou numéro\" autocomplete=\"off\">\n                    <div class=\"matchs\">\n                        <div v-for=\"(match, index) in matchs\" :key=\"index\" @click=\"addSearch(match)\">{{ match.traduction }}</div>\n                    </div>\n                </div>\n                <button type=\"submit\"><img src=\"../assets/search.png\" alt=\"search-icon\"></button>\n            </form>\n        </section>\n        <section class=\"section pokedex-results\">\n            <ul class=\"results\">\n                <li v-for=\"(pokemon, index) in pokemons\" :key=\"index\">\n                    <PokeCard :pokemon=\"pokemon\"></PokeCard>\n                </li>\n            </ul>\n            <div class=\"load-more\">\n                <button href=\"#\" @click=\"generateMorePokemons\" v-show=\"active\">Charger d'autres Pokémon</button>\n            </div>\n        </section>    \n    </div>\n</template>\n\n<script setup>\n// import { ref, computed, onUnmounted } from 'vue'\nimport { ref, computed } from 'vue'\nimport { useStore } from 'vuex'\nimport PokeCard from '../components/PokeCard.vue'\nimport { watch } from 'vue'\n\nconst searchTerm = ref('')\nconst active = ref(true)\nconst store = useStore()\nconst pokemons = computed(() => {  return store.state.pokemons})\nconst matchs = computed(() => {  return store.state.matchs})\n\nstore.dispatch('generatePokemons')\n// store.dispatch('addPokemons', { reset: true, search: null })\n\nfunction generateMorePokemons() {\n    active.value = false\n    store.dispatch('generatePokemons')\n    window.addEventListener('scroll', loadOnScroll)\n}\nfunction loadOnScroll() {\n    if (window.scrollY >= window.scrollMaxY) {\n        generateMorePokemons()\n    }\n}\n// function addSearch(name) {\n//     searchTerm.value = name\n//     store.dispatch('emptyMatchedNames')\n// }\n// function onSubmit(event) {\n//     event.preventDefault()\n//     store.dispatch('addPokemons', { reset: true, search: searchTerm.value })\n//     if (searchTerm.value === '') {\n//         active.value = true\n//         window.removeEventListener('scroll', loadOnScroll)\n//     } else {\n//         active.value = false\n//     }\n// }\n\nwatch(searchTerm, (name) => {\n    if (name === '') {\n        // store.dispatch('emptyMatchedNames')\n    } else {\n        store.dispatch('generateMatchsPokemons', name)\n    }\n})\n\n// // onMounted(() => {  window.addEventListener('scroll', loadOnScroll)})\n// onUnmounted(() => {  window.removeEventListener('scroll', loadOnScroll)}) \n</script>\n\n<style scoped>\n.pokedex {\n    max-width: 1280px;\n    margin: 0 auto;\n}\n.section {\n    padding-top: 2em;\n    padding-bottom: 2em;\n    color: #FFF;\n}\n.pokedex-header,\n.pokedex-filter {\n   padding-right: 3em;\n   padding-left: 3em;\n}\n.pokedex-header h1 {\n    font-size: 3em;\n    font-weight: 600;\n}\n.pokedex-filter form {\n    position: relative;\n    display: flex;\n    align-items: flex-end;\n}\n.pokedex-filter form div {\n    display: flex;\n    flex-direction: column;\n    position: relative;\n}\n.pokedex-filter form div label {\n    margin-bottom: 0.8em;\n    font-size: 24px;\n}\ninput[type=text] {\n    height: 50px;\n    width: 100%;\n    min-width: 300px;\n    background-color: #ebebeb;\n    border: none;\n    border-radius: 5px;\n    padding: 1em;\n    line-height: 1.5;\n    cursor: pointer;\n}\ninput[type=text]::placeholder {\n    color: #6d6d6d;\n}\ninput[type=text]:focus-visible {\n    outline: auto;\n}\nbutton[type=submit] {\n    width: 50px;\n    height: 50px;\n    margin-left: 0.5em;\n    border-radius: 5px;\n    border: none;\n    background-color: #ff1a1a;\n    cursor: pointer;\n}\nbutton[type=submit] img {\n    width: 24px;\n    transform: translateY(2px);\n}\n.matchs {\n    position: absolute !important;\n    background-color: #ebebeb;\n    top: 100%;\n    right: 0;\n    left: 0;\n    z-index: 1;\n}\n.matchs div {\n    box-sizing: content-box;\n    height: 100%;\n    color: #000;\n    padding: 1em;\n}\n.matchs div:hover {\n    background-color: #ff1a1a;\n}\n.pokedex-results .results {\n    list-style: none;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n}\n.pokedex-results .results li {\n    margin: 0.8em;\n}\n.pokedex-results .load-more {\n    display: grid;\n    justify-content: center;\n    margin: 4em 0;\n}\n.pokedex-results .load-more button {\n    text-decoration: none;\n    color: #FFF;\n    font-weight: 600;\n    font-size: 18px;\n    padding: 0.75em 1.25em 0.675em;\n    border-radius: 5px;\n    border: none;\n    background-color: #ff1a1a;\n    cursor: pointer;\n}\n</style>"]},"metadata":{},"sourceType":"module"}