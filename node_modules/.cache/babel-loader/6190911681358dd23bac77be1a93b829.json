{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport PokemonCard from '../components/PokemonCard.vue';\nexport default {\n  name: 'PokemonList',\n  components: {\n    PokemonCard\n  },\n  data() {\n    return {\n      pokemons: []\n    };\n  },\n  methods: {\n    fetchPokemons() {\n      const Pokedex = require(\"pokeapi-js-wrapper\");\n      const P = new Pokedex.Pokedex();\n      const interval = {\n        offset: 0,\n        limit: 10\n      };\n      P.getPokemonsList(interval).then(function (response) {\n        response.results.forEach(pokemon => {\n          P.getPokemonByName(pokemon.name).then(function (pokemonDetail) {\n            // console.log(pokemonDetail)\n            this.pokemons.push(pokemonDetail);\n            console.log(this.pokemons);\n          });\n        });\n      });\n    }\n  },\n  created() {\n    this.fetchPokemons();\n  }\n};","map":{"version":3,"mappings":";AASA,OAAOA,WAAU,MAAO,+BAA8B;AAEtD,eAAe;EACXC,IAAI,EAAE,aAAa;EACnBC,UAAU,EAAE;IACRF;EACJ,CAAC;EACDG,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,OAAO,EAAE;IACLC,aAAa,GAAG;MACZ,MAAMC,OAAM,GAAIC,OAAO,CAAC,oBAAoB;MAC5C,MAAMC,IAAI,IAAIF,OAAO,CAACA,OAAO,EAAC;MAC9B,MAAMG,QAAO,GAAI;QACbC,MAAM,EAAE,CAAC;QACTC,KAAK,EAAE;MACX;MACAH,CAAC,CAACI,eAAe,CAACH,QAAQ,CAAC,CAACI,IAAI,CAAC,UAASC,QAAQ,EAAE;QAChDA,QAAQ,CAACC,OAAO,CAACC,OAAO,CAACC,OAAM,IAAK;UAChCT,CAAC,CAACU,gBAAgB,CAACD,OAAO,CAACjB,IAAI,CAAC,CAACa,IAAI,CAAC,UAASM,aAAa,EAAE;YAC1D;YACA,IAAI,CAAChB,QAAQ,CAACiB,IAAI,CAACD,aAAa;YAChCE,OAAO,CAACC,GAAG,CAAC,IAAI,CAACnB,QAAQ;UAC7B,CAAC;QAEL,CAAC,CAAC;MACN,CAAC;IACL;EACJ,CAAC;EACDoB,OAAO,GAAG;IACN,IAAI,CAAClB,aAAa,EAAC;EACvB;AACJ","names":["PokemonCard","name","components","data","pokemons","methods","fetchPokemons","Pokedex","require","P","interval","offset","limit","getPokemonsList","then","response","results","forEach","pokemon","getPokemonByName","pokemonDetail","push","console","log","created"],"sourceRoot":"","sources":["/home/pierrots/polytech/WOA/js/lab4/pokedex/src/views/PokemonList.vue"],"sourcesContent":["<template>\n    <ul>\n        <li>\n            <PokemonCard v-for=\"(pokemon, index) in pokemons\" :key=\"index\" :pokemon=\"pokemon\" />\n        </li>\n    </ul>\n</template>\n\n<script>\nimport PokemonCard from '../components/PokemonCard.vue'\n\nexport default {\n    name: 'PokemonList',\n    components: {\n        PokemonCard\n    },\n    data() {\n        return {\n            pokemons: []\n        }\n    },\n    methods: {\n        fetchPokemons() {\n            const Pokedex = require(\"pokeapi-js-wrapper\")\n            const P = new Pokedex.Pokedex()\n            const interval = {\n                offset: 0,\n                limit: 10,\n            }\n            P.getPokemonsList(interval).then(function(response) {\n                response.results.forEach(pokemon => {\n                    P.getPokemonByName(pokemon.name).then(function(pokemonDetail) {\n                        // console.log(pokemonDetail)\n                        this.pokemons.push(pokemonDetail)\n                        console.log(this.pokemons)\n                    })\n                    \n                });\n            })\n        }\n    },\n    created() {\n        this.fetchPokemons()\n    }\n}\n</script>\n\n<style>\n\n</style> "]},"metadata":{},"sourceType":"module"}