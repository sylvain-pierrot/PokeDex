=== '') {\n        store.dispatch('emptyMatchedNames')\n    } else {\n        store.dispatch('matchPokemons', name)\n    }\n})\nfunction addMorePokemons() {\n    active.value = false\n    store.dispatch('addPokemons', { reset: false, search: null })\n    window.addEventListener('scroll', loadOnScroll)\n}\nfunction loadOnScroll() {\n    if (window.scrollY >= window.scrollMaxY) {\n        addMorePokemons()\n    }\n}\nfunction addSearch(name) {\n    searchTerm.value = name\n    store.dispatch('emptyMatchedNames')\n    window.removeEventListener('scroll', loadOnScroll)\n}\nfunction onSubmit(event) {\n    event.preventDefault()\n    store.dispatch('addPokemons', { reset: true, search: searchTerm.value })\n    active.value = false\n}\n\n// onMounted(() => {  window.addEventListener('scroll', loadOnScroll)})\nonUnmounted(() => {  window.removeEventListener('scroll', loadOnScroll)}) \n</script>\n\n<style scoped>\n.pokedex {\n    max-width: 1280px;\n    margin: 0 auto;\n}\n.section {\n    padding-top: 2em;\n    padding-bottom: 2em;\n    color: #FFF;\n}\n.pokedex-header,\n.pokedex-filter {\n   padding-right: 3em;\n   padding-left: 3em;\n}\n.pokedex-header h1 {\n    font-size: 3em;\n    font-weight: 600;\n}\n.pokedex-filter form {\n    position: relative;\n    display: flex;\n    align-items: flex-end;\n}\n.pokedex-filter form div {\n    display: flex;\n    flex-direction: column;\n    position: relative;\n}\n.pokedex-filter form div label {\n    margin-bottom: 0.8em;\n    font-size: 24px;\n}\ninput[type=text] {\n    height: 50px;\n    width: 100%;\n    min-width: 300px;\n    background-color: #ebebeb;\n    border: none;\n    border-radius: 5px;\n    padding: 1em;\n    line-height: 1.5;\n    cursor: pointer;\n}\ninput[type=text]::placeholder {\n    color: #6d6d6d;\n}\ninput[type=text]:focus-visible {\n    outline: auto;\n}\nbutton[type=submit] {\n    width: 50px;\n    height: 50px;\n    margin-left: 0.5em;\n    border-radius: 5px;\n    border: none;\n    background-color: #ff1a1a;\n    cursor: pointer;\n}\nbutton[type=submit] img {\n    width: 24px;\n    transform: translateY(2px);\n}\n.matchs {\n    position: absolute !important;\n    background-color: #ebebeb;\n    top: 100%;\n    right: 0;\n    left: 0;\n    z-index: 1;\n}\n.matchs div {\n    box-sizing: content-box;\n    height: 100%;\n    color: #000;\n    padding: 1em;\n}\n.matchs div:hover {\n    background-color: #ff1a1a;\n}\n.pokedex-results .results {\n    list-style: none;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n}\n.pokedex-results .results li {\n    margin: 0.8em;\n}\n.pokedex-results .load-more {\n    display: grid;\n    justify-content: center;\n    margin: 4em 0;\n}\n.pokedex-results .load-more button {\n    text-decoration: none;\n    color: #FFF;\n    font-weight: 600;\n    font-size: 18px;\n    padding: 0.75em 1.25em 0.675em;\n    border-radius: 5px;\n    border: none;\n    background-color: #ff1a1a;\n    cursor: pointer;\n}\n</style>"]},"metadata":{},"sourceType":"module"}