{"ast":null,"code":"import { ref, computed, onUnmounted } from 'vue';\nimport { useStore } from 'vuex';\nimport PokeCard from '../components/PokeCard.vue';\nimport { watch } from 'vue';\nexport default {\n  __name: 'HomePage',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const searchTerm = ref('');\n    const active = ref(true);\n    const store = useStore();\n    const pokemons = computed(() => {\n      return store.state.pokemons;\n    });\n    const matchedNames = computed(() => {\n      return store.state.matchedNames;\n    });\n    store.dispatch('addPokemons', {\n      reset: true,\n      search: null\n    });\n    watch(searchTerm, name => {\n      if (name === '') {\n        store.dispatch('emptyMatchedNames');\n      } else {\n        store.dispatch('matchPokemons', name);\n      }\n    });\n    function addMorePokemons() {\n      active.value = false;\n      store.dispatch('addPokemons', {\n        reset: false,\n        search: null\n      });\n      window.addEventListener('scroll', loadOnScroll);\n    }\n    function loadOnScroll() {\n      if (window.scrollY >= window.scrollMaxY) {\n        addMorePokemons();\n      }\n    }\n    function addSearch(name) {\n      searchTerm.value = name;\n      store.dispatch('emptyMatchedNames');\n      window.removeEventListener('scroll', loadOnScroll);\n    }\n    function onSubmit(event) {\n      event.preventDefault();\n      store.dispatch('addPokemons', {\n        reset: true,\n        search: searchTerm.value\n      });\n      active.value = false;\n    }\n\n    // onMounted(() => {  window.addEventListener('scroll', loadOnScroll)})\n    onUnmounted(() => {\n      window.removeEventListener('scroll', loadOnScroll);\n    });\n    const __returned__ = {\n      searchTerm,\n      active,\n      store,\n      pokemons,\n      matchedNames,\n      addMorePokemons,\n      loadOnScroll,\n      addSearch,\n      onSubmit,\n      ref,\n      computed,\n      onUnmounted,\n      useStore,\n      PokeCard,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AA+BA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,WAAW,QAAQ,KAAK;AAChD,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,SAASC,KAAK,QAAQ,KAAK;;;;;;;IAE3B,MAAMC,UAAU,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMO,MAAM,GAAGP,GAAG,CAAC,IAAI,CAAC;IACxB,MAAMQ,KAAK,GAAGL,QAAQ,EAAE;IACxB,MAAMM,QAAQ,GAAGR,QAAQ,CAAC,MAAM;MAAG,OAAOO,KAAK,CAACE,KAAK,CAACD,QAAQ;IAAA,CAAC,CAAC;IAChE,MAAME,YAAY,GAAGV,QAAQ,CAAC,MAAM;MAAG,OAAOO,KAAK,CAACE,KAAK,CAACC,YAAY;IAAA,CAAC,CAAC;IAExEH,KAAK,CAACI,QAAQ,CAAC,aAAa,EAAE;MAAEC,KAAK,EAAE,IAAI;MAAEC,MAAM,EAAE;IAAK,CAAC,CAAC;IAE5DT,KAAK,CAACC,UAAU,EAAGS,IAAI,IAAK;MACxB,IAAIA,IAAI,KAAK,EAAE,EAAE;QACbP,KAAK,CAACI,QAAQ,CAAC,mBAAmB,CAAC;MACvC,CAAC,MAAM;QACHJ,KAAK,CAACI,QAAQ,CAAC,eAAe,EAAEG,IAAI,CAAC;MACzC;IACJ,CAAC,CAAC;IACF,SAASC,eAAe,GAAG;MACvBT,MAAM,CAACU,KAAK,GAAG,KAAK;MACpBT,KAAK,CAACI,QAAQ,CAAC,aAAa,EAAE;QAAEC,KAAK,EAAE,KAAK;QAAEC,MAAM,EAAE;MAAK,CAAC,CAAC;MAC7DI,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAEC,YAAY,CAAC;IACnD;IACA,SAASA,YAAY,GAAG;MACpB,IAAIF,MAAM,CAACG,OAAO,IAAIH,MAAM,CAACI,UAAU,EAAE;QACrCN,eAAe,EAAE;MACrB;IACJ;IACA,SAASO,SAAS,CAACR,IAAI,EAAE;MACrBT,UAAU,CAACW,KAAK,GAAGF,IAAI;MACvBP,KAAK,CAACI,QAAQ,CAAC,mBAAmB,CAAC;MACnCM,MAAM,CAACM,mBAAmB,CAAC,QAAQ,EAAEJ,YAAY,CAAC;IACtD;IACA,SAASK,QAAQ,CAACC,KAAK,EAAE;MACrBA,KAAK,CAACC,cAAc,EAAE;MACtBnB,KAAK,CAACI,QAAQ,CAAC,aAAa,EAAE;QAAEC,KAAK,EAAE,IAAI;QAAEC,MAAM,EAAER,UAAU,CAACW;MAAM,CAAC,CAAC;MACxEV,MAAM,CAACU,KAAK,GAAG,KAAK;IACxB;;IAEA;IACAf,WAAW,CAAC,MAAM;MAAGgB,MAAM,CAACM,mBAAmB,CAAC,QAAQ,EAAEJ,YAAY,CAAC;IAAA,CAAC,CAAC","names":["ref","computed","onUnmounted","useStore","PokeCard","watch","searchTerm","active","store","pokemons","state","matchedNames","dispatch","reset","search","name","addMorePokemons","value","window","addEventListener","loadOnScroll","scrollY","scrollMaxY","addSearch","removeEventListener","onSubmit","e