{"ast":null,"code":"import { ref, computed, onUnmounted } from 'vue';\nimport { useStore } from 'vuex';\nimport PokeCard from '../components/PokeCard.vue';\nimport { watch } from 'vue';\nexport default {\n  __name: 'PokedexPage',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const searchTerm = ref('');\n    const store = useStore();\n    const pokemons = computed(() => {\n      return store.state.pokemons;\n    });\n    const matchs = computed(() => {\n      return store.state.matchs;\n    });\n    store.dispatch('initCacheNames');\n    store.dispatch('generatePokemons');\n    window.addEventListener('scroll', generateMorePokemons);\n    function generateMorePokemons() {\n      if (window.scrollY >= window.scrollMaxY) {\n        store.dispatch('generatePokemons');\n      }\n    }\n    function generatePokemonsBySearch(name) {\n      searchTerm.value = name;\n      const evt = new CustomEvent(\"submit\", {\n        \"bubbles\": true,\n        \"cancelable\": true\n      });\n      document.getElementById('form').dispatchEvent(evt);\n    }\n    function onSubmit(event) {\n      event.preventDefault();\n      if (searchTerm.value === '') {\n        window.addEventListener('scroll', generateMorePokemons);\n      } else {\n        window.removeEventListener('scroll', generateMorePokemons);\n      }\n      store.dispatch('generatePokemonsBySearch', searchTerm.value);\n    }\n    function generateLegendary() {\n      store.dispatch('generateAllLegendary');\n    }\n    watch(searchTerm, name => {\n      store.dispatch('generateMatchsPokemons', name);\n    });\n\n    // onMounted(() => {  window.addEventListener('scroll', loadOnScroll)})\n    onUnmounted(() => {\n      window.removeEventListener('scroll', generateMorePokemons);\n    });\n    const __returned__ = {\n      searchTerm,\n      store,\n      pokemons,\n      matchs,\n      generateMorePokemons,\n      generatePokemonsBySearch,\n      onSubmit,\n      generateLegendary,\n      ref,\n      computed,\n      onUnmounted,\n      useStore,\n      PokeCard,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AA6BA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,WAAW,QAAQ,KAAK;AAChD,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,SAASC,KAAK,QAAQ,KAAK;;;;;;;IAE3B,MAAMC,UAAU,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMO,KAAK,GAAGJ,QAAQ,EAAE;IACxB,MAAMK,QAAQ,GAAGP,QAAQ,CAAC,MAAM;MAAG,OAAOM,KAAK,CAACE,KAAK,CAACD,QAAQ;IAAA,CAAC,CAAC;IAChE,MAAME,MAAM,GAAGT,QAAQ,CAAC,MAAM;MAAG,OAAOM,KAAK,CAACE,KAAK,CAACC,MAAM;IAAA,CAAC,CAAC;IAE5DH,KAAK,CAACI,QAAQ,CAAC,gBAAgB,CAAC;IAChCJ,KAAK,CAACI,QAAQ,CAAC,kBAAkB,CAAC;IAElCC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAEC,oBAAoB,CAAC;IAEvD,SAASA,oBAAoB,GAAG;MAC5B,IAAIF,MAAM,CAACG,OAAO,IAAIH,MAAM,CAACI,UAAU,EAAE;QAAET,KAAK,CAACI,QAAQ,CAAC,kBAAkB,CAAC;MAAA;IACjF;IACA,SAASM,wBAAwB,CAACC,IAAI,EAAE;MACpCZ,UAAU,CAACa,KAAK,GAAGD,IAAI;MACvB,MAAME,GAAG,GAAG,IAAIC,WAAW,CAAC,QAAQ,EAAE;QAAC,SAAS,EAAE,IAAI;QAAE,YAAY,EAAE;MAAI,CAAC,CAAC;MAC5EC,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,aAAa,CAACJ,GAAG,CAAC;IACtD;IACA,SAASK,QAAQ,CAACC,KAAK,EAAE;MACrBA,KAAK,CAACC,cAAc,EAAE;MACtB,IAAIrB,UAAU,CAACa,KAAK,KAAK,EAAE,EAAE;QACzBP,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAEC,oBAAoB,CAAC;MAC3D,CAAC,MAAM;QACHF,MAAM,CAACgB,mBAAmB,CAAC,QAAQ,EAAEd,oBAAoB,CAAC;MAC9D;MACAP,KAAK,CAACI,QAAQ,CAAC,0BAA0B,EAAEL,UAAU,CAACa,KAAK,CAAC;IAChE;IACA,SAASU,iBAAiB,GAAG;MACzBtB,KAAK,CAACI,QAAQ,CAAC,sBAAsB,CAAC;IAC1C;IAEAN,KAAK,CAACC,UAAU,EAAGY,IAAI,IAAK;MACxBX,KAAK,CAACI,QAAQ,CAAC,wBAAwB,EAAEO,IAAI,CAAC;IAClD,CAAC,CAAC;;IAEF;IACAhB,WAAW,CAAC,MAAM;MAAGU,MAAM,CAACgB,mBAAmB,CAAC,QAAQ,EAAEd,oBAAoB,CAAC;IAAA,CAAC,CAAC","names":["ref","computed","onUnmounted","useStore","PokeCard","watch","searchTerm","store","pokemons","state","matchs","dispatch","window","addEventListener","generateMorePokemons","scrollY","scrollMaxY","generatePokemonsBySearch","name","value","evt","CustomEvent","document","getElementById","dispatchEvent","onSubmit","event","preventDefault","removeEventListener","generateLegendary"],"sources":["/home/pierrots/Polytech/WOA/js/lab4/pokedex/src/views/PokedexPage.vue"],"sourcesContent":["<template>\n    <div class=\"pokedex\">\n        <section class=\"section pokedex-header\">\n            <h1>POKEDEX</h1>\n        </section>\n        <section class=\"section pokedex-filter\">\n            <form id=\"form\" v-on:submit.prevent=\"onSubmit\">\n                <div>\n                    <label for=\"search\">Quel Pokemon cherchez-vous ?</label>\n                    <input v-model=\"searchTerm\" type=\"text\" id=\"search\" name=\"search\" placeholder=\"Nom ou numÃ©ro\" autocomplete=\"off\">\n                    <div class=\"matchs\">\n                        <div v-for=\"(match, index) in matchs\" :key=\"index\" @click=\"generatePokemonsBySearch(match.traduction.toLowerCase())\">{{ match.traduction.toLowerCase() }}</div>\n                    </div>\n                </div>\n                <button type=\"submit\"><img src=\"../assets/search.png\" alt=\"search-icon\"></button>\n            </form>\n            <button @click=\"generateLegendary\">LEGENDARY</button>\n        </section>\n        <section class=\"section pokedex-results\">\n            <ul class=\"results\">\n                <li v-for=\"(pokemon, index) in pokemons\" :key=\"index\">\n                    <PokeCard :pokemon=\"pokemon\"></PokeCard>\n                </li>\n            </ul>\n        </section>    \n    </div>\n</template>\n\n<script setup>\nimport { ref, computed, onUnmounted } from 'vue'\nimport { useStore } from 'vuex'\nimport PokeCard from '../components/PokeCard.vue'\nimport { watch } from 'vue'\n\nconst searchTerm = ref('')\nconst store = useStore()\nconst pokemons = computed(() => {  return store.state.pokemons})\nconst matchs = computed(() => {  return store.state.matchs})\n\nstore.dispatch('initCacheNames')\nstore.dispatch('generatePokemons')\n\nwindow.addEventListener('scroll', generateMorePokemons)\n\nfunction generateMorePokemons() {\n    if (window.scrollY >= window.scrollMaxY) { store.dispatch('generatePokemons')}\n}\nfunction generatePokemonsBySearch(name) {\n    searchTerm.value = name\n    const evt = new CustomEvent(\"submit\", {\"bubbles\": true, \"cancelable\": true})\n    document.getElementById('form').dispatchEvent(evt)\n}\nfunction onSubmit(event) {\n    event.preventDefault()\n    if (searchTerm.value === '') {\n        window.addEventListener('scroll', generateMorePokemons)\n    } else {\n        window.removeEventListener('scroll', generateMorePokemons)\n    }\n    store.dispatch('generatePokemonsBySearch', searchTerm.value)\n}\nfunction generateLegendary() {\n    store.dispatch('generateAllLegendary')\n}\n\nwatch(searchTerm, (name) => {\n    store.dispatch('generateMatchsPokemons', name)\n})\n\n// onMounted(() => {  window.addEventListener('scroll', loadOnScroll)})\nonUnmounted(() => {  window.removeEventListener('scroll', generateMorePokemons)}) \n</script>\n\n<style scoped>\n.pokedex {\n    max-width: 1280px;\n    margin: 0 auto;\n}\n.section {\n    padding-top: 2em;\n    padding-bottom: 2em;\n    color: #FFF;\n}\n.pokedex-header,\n.pokedex-filter {\n   padding-right: 3em;\n   padding-left: 3em;\n}\n.pokedex-header h1 {\n    font-size: 3em;\n    font-weight: 600;\n}\n.pokedex-filter form {\n    position: relative;\n    display: flex;\n    align-items: flex-end;\n}\n.pokedex-filter form div {\n    display: flex;\n    flex-direction: column;\n    position: relative;\n}\n.pokedex-filter form div label {\n    margin-bottom: 0.8em;\n    font-size: 24px;\n}\ninput[type=text] {\n    height: 50px;\n    width: 100%;\n    min-width: 300px;\n    background-color: #ebebeb;\n    border: none;\n    border-radius: 5px;\n    padding: 1em;\n    line-height: 1.5;\n}\ninput[type=text]::placeholder {\n    color: #6d6d6d;\n    font-size: 16px;\n}\ninput[type=text]:focus-visible {\n    outline: auto;\n}\nbutton[type=submit] {\n    width: 50px;\n    height: 50px;\n    margin-left: 0.5em;\n    border-radius: 5px;\n    border: none;\n    background-color: #ff1a1a;\n    cursor: pointer;\n}\nbutton[type=submit] img {\n    width: 24px;\n    transform: translateY(2px);\n}\n.matchs {\n    position: absolute !important;\n    background-color: #ebebeb;\n    top: 100%;\n    right: 0;\n    left: 0;\n    z-index: 1;\n}\n.matchs div {\n    box-sizing: content-box;\n    height: 100%;\n    color: #000;\n    padding: 1em;\n    cursor: pointer;\n}\n.matchs div:hover {\n    background-color: #ff1a1a;\n}\n.pokedex-results .results {\n    list-style: none;\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n}\n.pokedex-results .results li {\n    margin: 0.8em;\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}