{"ast":null,"code":"import { ref, computed } from 'vue';\nimport { useStore } from 'vuex';\nimport PokeCard from '../components/PokeCard.vue';\nimport { watch } from 'vue';\nexport default {\n  __name: 'HomePage',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const searchTerm = ref('');\n    const selectedPoke = ref('');\n    const store = useStore();\n    const pokemon = computed(() => {\n      return store.state.currentPokemon[0];\n    });\n    const pokemonsMatched = computed(() => {\n      return store.state.pokemonsMatched;\n    });\n    watch(searchTerm, name => {\n      store.dispatch('matchPokemons', name);\n    });\n    function submit(event) {\n      event.preventDefault();\n      store.dispatch('addCurrentPokemon', searchTerm.value);\n    }\n    function selectPoke(name) {\n      selectedPoke.value = name;\n      searchTerm.value = '';\n      store.dispatch('addCurrentPokemon', selectedPoke.value);\n    }\n    const __returned__ = {\n      searchTerm,\n      selectedPoke,\n      store,\n      pokemon,\n      pokemonsMatched,\n      submit,\n      selectPoke,\n      ref,\n      computed,\n      useStore,\n      PokeCard,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AAmBA,SAASA,GAAG,EAAEC,QAAQ,QAAQ,KAAK;AACnC,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,SAASC,KAAK,QAAQ,KAAK;;;;;;;IAE3B,MAAMC,UAAU,GAAGL,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMM,YAAY,GAAGN,GAAG,CAAC,EAAE,CAAC;IAC5B,MAAMO,KAAK,GAAGL,QAAQ,EAAE;IACxB,MAAMM,OAAO,GAAGP,QAAQ,CAAC,MAAM;MAAG,OAAOM,KAAK,CAACE,KAAK,CAACC,cAAc,CAAC,CAAC,CAAC;IAAA,CAAC,CAAC;IACxE,MAAMC,eAAe,GAAGV,QAAQ,CAAC,MAAM;MAAG,OAAOM,KAAK,CAACE,KAAK,CAACE,eAAe;IAAA,CAAC,CAAC;IAE9EP,KAAK,CAACC,UAAU,EAAGO,IAAI,IAAK;MAC1BL,KAAK,CAACM,QAAQ,CAAC,eAAe,EAAED,IAAI,CAAC;IACvC,CAAC,CAAC;IAEF,SAASE,MAAM,CAACC,KAAK,EAAE;MACnBA,KAAK,CAACC,cAAc,EAAE;MACtBT,KAAK,CAACM,QAAQ,CAA