    searchTerm.value = ''\n    store.dispatch('addCurrentPokemon', selectedPoke.value)\n}\n\n</script>\n\n<style scoped>\n.matchs {\n    position: absolute;\n    list-style: none;\n}\n.matchs > li {\n    width: 255px;\n    background-color: #ebebeb;\n    border: none;\n    padding-left: 44px;\n    cursor: pointer;\n    text-align: center;\n    padding: 2px 8px;\n    outline: 1px solid rgba(76,87,102,0.1);\n}\n/* header */\n#header {\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}\n#header .title {\n    margin: 1.5em 25px;\n    font-size: 2em;\n    font-weight: 600;\n    color: #FFF;\n}\n#header .form_pokemon {\n    position: relative;\n}\n#header .form_pokemon label {\n    position: absolute;\n    left: 14px;\n    top: 50%;\n    transform: translateY(-50%);\n    display: grid;\n}\n#header .form_pokemon label>img {\n    width: 24px;    \n}\n#header .form_pokemon input {\n    height: 50px;\n    width: 255px;\n    background-color: #ebebeb;\n    border: none;\n    border-radius: 25px;\n    padding-left: 44px;\n    cursor: pointer;\n}\n#header .form_pokemon input::placeholder {\n    color: #6d6d6d;\n}\n#header .form_pokemon input:focus-visible {\n    outline: 1px solid rgba(76,87,102,0.1);   \n}\n.card-searched {\n    margin: 5em 0;\n    display: grid;\n    justify-content: center;\n}\n\n</style>"]},"metadata":{},"sourceType":"module"}