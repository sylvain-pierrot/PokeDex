{"ast":null,"code":"import { computed } from 'vue';\nimport { useStore } from 'vuex';\nimport { useRoute } from 'vue-router';\nimport PokeCard from '../components/PokeCard.vue';\nexport default {\n  __name: 'HomePage',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const route = useRoute();\n    const store = useStore();\n    store.dispatch('addCurrentPokemonById', name);\n    const pokemon = computed(() => {\n      return store.state.currentPokemon[0];\n    });\n    const __returned__ = {\n      route,\n      store,\n      pokemon,\n      computed,\n      useStore,\n      useRoute,\n      PokeCard\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AAYA,SAASA,QAAQ,QAAQ,KAAK;AAC9B,SAASC,QAAQ,QAAQ,MAAM;AAC/B,SAASC,QAAQ,QAAQ,YAAY;AACrC,OAAOC,QAAQ,MAAM,4BAA4B;;;;;;;IAEjD,MAAMC,KAAK,GAAGF,QAAQ,EAAE;IACxB,MAAMG,KAAK,GAAGJ,QAAQ,EAAE;IAExBI,KAAK,CAACC,QAAQ,CAAC,uBAAuB,EAAEC,IAAI,CAAC;IAC7C,MAAMC,OAAO,GAAGR,QAAQ,CAAC,MAAM;MAAG,OAAOK,KAAK,CAACI,KAAK,CAACC,cAAc,CAAC,CAAC,CAAC;IAAA,CAAC,CAAC","names":["computed","useStore","useRoute","PokeCard","route","store","dispatch","name","pokemon","state","currentPokemon"],"sources":["/home/pierrots/Polytech/WOA/js/lab4/pokedex/src/views/HomePage.vue"],"sourcesContent":["<template>\n    <section id=\"header\">\n        <h1 class=\"title\">Quel Pokemon cherchez-vous?</h1>\n        <form class=\"form_pokemon\" action=\"\">\n            <label for=\"search_pokemon\"><img src=\"../assets/search.png\" alt=\"search_icon\"></label>\n            <input v-model=\"name\" type=\"text\" id=\"search_pokemon\" placeholder=\"Chercher un Pokemon\">\n        </form>\n    </section>\n    <PokeCard v-if=\"name\" :pokemon=\"pokemon\"></PokeCard>\n</template>\n\n<script setup>\nimport { computed } from 'vue'\nimport { useStore } from 'vuex'\nimport { useRoute } from 'vue-router'\nimport PokeCard from '../components/PokeCard.vue'\n\nconst route = useRoute()\nconst store = useStore()\n\nstore.dispatch('addCurrentPokemonById', name)\nconst pokemon = computed(() => {  return store.state.currentPokemon[0]})\n</script>\n\n<style scoped>\n/* header */\n#header {\n    position: relative;\n    display: flex;\n    flex-direction: column;\n    justify-content: center;\n    align-items: center;\n}\n#header .title {\n    margin: 1.5em 25px;\n    font-size: 2em;\n    font-weight: 600;\n    color: #FFF;\n}\n#header .form_pokemon {\n    position: relative;\n}\n#header .form_pokemon label {\n    position: absolute;\n    left: 14px;\n    top: 50%;\n    transform: translateY(-50%);\n    display: grid;\n}\n#header .form_pokemon label>img {\n    width: 24px;    \n}\n#header .form_pokemon input {\n    height: 50px;\n    width: 255px;\n    background-color: #ebebeb;\n    border: none;\n    border-radius: 25px;\n    padding-left: 44px;\n    cursor: pointer;\n}\n#header .form_pokemon input::placeholder {\n    color: #6d6d6d;\n}\n#header .form_pokemon input:focus-visible {\n    outline: 1px solid rgba(76,87,102,0.1);\n    \n}\n\n</style>"]},"metadata":{},"sourceType":"module"}