{"ast":null,"code":"import { ref, computed, onUnmounted, onMounted } from \"vue\";\nimport { useStore } from \"vuex\";\nimport PokeCard from \"../components/PokeCard.vue\";\nimport { watch } from \"vue\";\nexport default {\n  __name: 'PokedexPage',\n  setup(__props, {\n    expose\n  }) {\n    expose();\n    const searchTerm = ref(\"\");\n    const timer = ref(null);\n    const store = useStore();\n    const pokemons = computed(() => {\n      return store.state.pokemons;\n    });\n    const matchs = computed(() => {\n      return store.state.matchs;\n    });\n    async function generateMorePokemons() {\n      if (window.scrollY >= window.scrollMaxY) {\n        await store.dispatch(\"generatePokedex\", false);\n      }\n    }\n    function generatePokedexBySearch(name) {\n      searchTerm.value = name;\n      const evt = new CustomEvent(\"submit\", {\n        bubbles: true,\n        cancelable: true\n      });\n      document.getElementById(\"form\").dispatchEvent(evt);\n    }\n    async function onSubmit(event) {\n      event.preventDefault();\n      if (searchTerm.value === \"\") {\n        window.addEventListener(\"scroll\", generateMorePokemons);\n      } else {\n        window.removeEventListener(\"scroll\", generateMorePokemons);\n      }\n      await store.dispatch(\"generatePokedexBySearch\", searchTerm.value);\n    }\n    async function all() {\n      await store.dispatch(\"generatePokedex\", true);\n      window.addEventListener(\"scroll\", generateMorePokemons);\n    }\n    watch(searchTerm, name => {\n      if (timer.value) {\n        clearTimeout(timer);\n        timer.value = null;\n      }\n      timer.value = setTimeout(async () => {\n        await store.dispatch(\"generateMatchsPokemons\", name);\n      }, 255);\n    });\n    onMounted(async () => {\n      await store.dispatch(\"generatePokedex\", true);\n      window.addEventListener(\"scroll\", generateMorePokemons);\n    });\n    onUnmounted(() => {\n      window.removeEventListener(\"scroll\", generateMorePokemons);\n    });\n    const __returned__ = {\n      searchTerm,\n      timer,\n      store,\n      pokemons,\n      matchs,\n      generateMorePokemons,\n      generatePokedexBySearch,\n      onSubmit,\n      all,\n      ref,\n      computed,\n      onUnmounted,\n      onMounted,\n      useStore,\n      PokeCard,\n      watch\n    };\n    Object.defineProperty(__returned__, '__isScriptSetup', {\n      enumerable: false,\n      value: true\n    });\n    return __returned__;\n  }\n};","map":{"version":3,"mappings":"AA4CA,SAASA,GAAG,EAAEC,QAAQ,EAAEC,WAAW,EAAEC,SAAS,QAAQ,KAAK;AAC3D,SAASC,QAAQ,QAAQ,MAAM;AAC/B,OAAOC,QAAQ,MAAM,4BAA4B;AACjD,SAASC,KAAK,QAAQ,KAAK;;;;;;;IAE3B,MAAMC,UAAU,GAAGP,GAAG,CAAC,EAAE,CAAC;IAC1B,MAAMQ,KAAK,GAAGR,GAAG,CAAC,IAAI,CAAC;IACvB,MAAMS,KAAK,GAAGL,QAAQ,EAAE;IACxB,MAAMM,QAAQ,GAAGT,QAAQ,CAAC,MAAM;MAC9B,OAAOQ,KAAK,CAACE,KAAK,CAACD,QAAQ;IAC7B,CAAC,CAAC;IACF,MAAME,MAAM,GAAGX,QAAQ,CAAC,MAAM;MAC5B,OAAOQ,KAAK,CAACE,KAAK,CAACC,MAAM;IAC3B,CAAC,CAAC;IAEF,eAAeC,oBAAoB,GAAG;MACpC,IAAIC,MAAM,CAACC,OAAO,IAAID,MAAM,CAACE,UAAU,EAAE;QACvC,MAAMP,KAAK,CAACQ,QAAQ,CAAC,iBAAiB,EAAE,KAAK,CAAC;MAChD;IACF;IACA,SAASC,uBAAuB,CAACC,IAAI,EAAE;MACrCZ,UAAU,CAACa,KAAK,GAAGD,IAAI;MACvB,MAAME,GAAG,GAAG,IAAIC,WAAW,CAAC,QAAQ,EAAE;QAAEC,OAAO,EAAE,IAAI;QAAEC,UAAU,EAAE;MAAK,CAAC,CAAC;MAC1EC,QAAQ,CAACC,cAAc,CAAC,MAAM,CAAC,CAACC,aAAa,CAACN,GAAG,CAAC;IACpD;IACA,eAAeO,QAAQ,CAACC,KAAK,EAAE;MAC7BA,KAAK,CAACC,cAAc,EAAE;MACtB,IAAIvB,UAAU,CAACa,KAAK,KAAK,EAAE,EAAE;QAC3BN,MAAM,CAACiB,gBAAgB,CAAC,QAAQ,EAAElB,oBAAoB,CAAC;MACzD,CAAC,MAAM;QACLC,MAAM,CAACkB,mBAAmB,CAAC,QAAQ,EAAEnB,oBAAoB,CAAC;MAC5D;MACA,MAAMJ,KAAK,CAACQ,QAAQ,CAAC,yBAAyB,EAAEV,UAAU,CAACa,KAAK,CAAC;IACnE;IACA,eAAea,GAAG,GAAG;MACnB,MAAMxB,KAAK,CAACQ,QAAQ,CAAC,iBAAiB,EAAE,IAAI,CAAC;MAC7CH,MAAM,CAACiB,gBAAgB,CAAC,QAAQ,EAAElB,oBAAoB,CAAC;IACzD;IAEAP,KAAK,CAACC,UAAU,EAAGY,IAAI,IAAK;MAC1B,IAAIX,KAAK,CAACY,KAAK,EAAE;QACfc,YAAY,CAAC1B,KAAK,CAAC;QACnBA,KAAK,CAACY,KAAK,GAAG,IAAI;MACpB;MACAZ,KAAK,CAACY,KAAK,GAAGe,UAAU,CAAC,YAAY;QACnC,M