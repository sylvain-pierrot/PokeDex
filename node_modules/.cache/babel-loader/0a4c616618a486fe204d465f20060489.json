{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapStates } from 'vuex';\nimport PokemonCard from '../components/PokemonCard.vue';\nexport default {\n  components: {\n    PokemonCard\n  },\n  data() {\n    return {\n      pokemons: []\n    };\n  },\n  mounted() {\n    const Pokedex = require(\"pokeapi-js-wrapper\");\n    const P = new Pokedex.Pokedex();\n    const interval = {\n      offset: 0,\n      limit: 10\n    };\n    const pokes = [];\n    P.getPokemonsList(interval).then(function (response) {\n      response.results.forEach(pokemon => {\n        P.getPokemonByName(pokemon.name).then(function (pokemonDetail) {\n          // console.log(pokemonDetail)\n          pokes.push(pokemonDetail);\n          console.log(pokes);\n        });\n      });\n    });\n    this.$store.commit('SET_POKEMONS', pokes);\n  }\n};","map":{"version":3,"mappings":";AASA,SAASA,SAAQ,QAAS,MAAK;AAC/B,OAAOC,WAAU,MAAO,+BAA8B;AAEtD,eAAe;EACXC,UAAU,EAAE;IACRD;EACJ,CAAC;EACDE,IAAI,GAAG;IACH,OAAO;MACHC,QAAQ,EAAE;IACd;EACJ,CAAC;EACDC,OAAO,GAAG;IACN,MAAMC,OAAM,GAAIC,OAAO,CAAC,oBAAoB;IAC5C,MAAMC,IAAI,IAAIF,OAAO,CAACA,OAAO,EAAC;IAC9B,MAAMG,QAAO,GAAI;MACbC,MAAM,EAAE,CAAC;MACTC,KAAK,EAAE;IACX;IACA,MAAMC,KAAI,GAAI,EAAC;IACfJ,CAAC,CAACK,eAAe,CAACJ,QAAQ,CAAC,CAACK,IAAI,CAAC,UAASC,QAAQ,EAAE;MAChDA,QAAQ,CAACC,OAAO,CAACC,OAAO,CAACC,OAAM,IAAK;QAChCV,CAAC,CAACW,gBAAgB,CAACD,OAAO,CAACE,IAAI,CAAC,CAACN,IAAI,CAAC,UAASO,aAAa,EAAE;UAC1D;UACAT,KAAK,CAACU,IAAI,CAACD,aAAa;UACxBE,OAAO,CAACC,GAAG,CAACZ,KAAK;QACrB,CAAC;MACL,CAAC,CAAC;IACN,CAAC;IACD,IAAI,CAACa,MAAM,CAACC,MAAM,CAAC,cAAc,EAAEd,KAAK;EAC5C;AACJ","names":["mapStates","PokemonCard","components","data","pokemons","mounted","Pokedex","require","P","interval","offset","limit","pokes","getPokemonsList","then","response","results","forEach","pokemon","getPokemonByName","name","pokemonDetail","push","console","log","$store","commit"],"sourceRoot":"","sources":["/home/pierrots/polytech/WOA/js/lab4/pokedex/src/views/PokemonList.vue"],"sourcesContent":["<template>\n    <ul>\n        <li>\n            <PokemonCard v-for=\"(pokemon, index) in pokemons\" :key=\"index\" :pokemon=\"pokemon\"></PokemonCard>\n        </li>\n    </ul>\n</template>\n\n<script>\nimport { mapStates } from 'vuex'\nimport PokemonCard from '../components/PokemonCard.vue'\n\nexport default {\n    components: {\n        PokemonCard\n    },\n    data() {\n        return {\n            pokemons: []\n        }\n    },\n    mounted() {\n        const Pokedex = require(\"pokeapi-js-wrapper\")\n        const P = new Pokedex.Pokedex()\n        const interval = {\n            offset: 0,\n            limit: 10,\n        }\n        const pokes = []\n        P.getPokemonsList(interval).then(function(response) {\n            response.results.forEach(pokemon => {\n                P.getPokemonByName(pokemon.name).then(function(pokemonDetail) {\n                    // console.log(pokemonDetail)\n                    pokes.push(pokemonDetail)\n                    console.log(pokes)\n                })\n            });\n        })\n        this.$store.commit('SET_POKEMONS', pokes) \n    }\n}\n</script>"]},"metadata":{},"sourceType":"module"}